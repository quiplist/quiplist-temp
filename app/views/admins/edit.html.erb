<%= render partial: "layouts/shared/breadcrumbs", locals: {
  links: [
    { path: admins_path, path_name: "Admins" },
    { path: admin_path(@admin), path_name: "#{@admin.id}" }
  ],
  current_path: "Edit"
} %>
<div class="container content-wrapper">
  <div class="row mb-3">
    <div class="col-12 text-start">
      <h2 class="event_title">Edit Admin</h2>
    </div>
    <hr/>
  </div>
    <%= form_for @admin, as: :admin, url: admin_path(@admin), method: :put, html: { class: "", multipart: true } do |f| %>
      <%= render partial: "layouts/shared/success_messages", locals: { messages: flash[:notice] } %>
      <%= render partial: "layouts/shared/error_messages", locals: { messages: @admin.errors.full_messages } %>
      <%= render partial: "admins/shared/form", locals: {f: f, with_password: false} %>
      <div class="row">
        <div class="col-12 text-end">
          <%= submit_tag "Edit Admin", class: "btn btn-submit" %>
        </div>
      </div>
    <% end %>
</div>
